{
  "name": "@craby/cli",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "bin": {
    "craby": "./bin.js"
  },
  "scripts": {
    "dev": "yarn build:debug && ./bin.js",
    "artifacts": "napi artifacts",
    "build": "yarn build:napi && tsdown",
    "build:debug": "yarn build:napi:debug && tsdown",
    "build:napi": "napi build --platform --release napi",
    "build:napi:debug": "napi build --platform napi",
    "prepublishOnly": "napi prepublish -t npm",
    "version": "napi version",
    "test": "vitest --run"
  },
  "files": [
    "bin.js",
    "dist",
    "template",
    "napi",
    "!napi/*.node"
  ],
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@react-native/codegen": "^0.80.1",
    "commander": "^14.0.0",
    "es-toolkit": "^1.39.7",
    "glob": "^11.0.3"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4",
    "@types/node": "^22",
    "tsdown": "^0.12.9",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">= 18"
  },
  "napi": {
    "name": "cli",
    "triples": {
      "additional": [
        "x86_64-apple-darwin",
        "x86_64-pc-windows-msvc",
        "x86_64-unknown-linux-gnu",
        "x86_64-unknown-linux-musl",
        "i686-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc"
      ]
    }
  }
}
